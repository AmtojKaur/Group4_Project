<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/rectangle"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.appcompat.widget.SearchView
        android:id="@+id/searchview_bar"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/rounded_rectangle_10dp"
        android:backgroundTint="@color/color_green1"
        android:iconifiedByDefault="false"
        android:queryHint="Search in chat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/img_settings"
        app:layout_constraintTop_toTopOf="parent"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_settings"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/rounded_rectangle_24dp"
        android:backgroundTint="@color/color_green1"
        android:src="@drawable/ic_settings_black_24dp"
        android:tint="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_profile"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:background="@drawable/rounded_rectangle_10dp"
        android:backgroundTint="@color/color_green1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/searchview_bar" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_profile_with_icon"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:background="@drawable/rounded_rectangle_10dp"
        android:backgroundTint="@color/color_green1"
        android:src="@drawable/ic_profile_black_24dp"
        app:layout_constraintStart_toEndOf="@id/img_profile"
        app:layout_constraintTop_toBottomOf="@id/searchview_bar" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/textview_blank"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:background="@drawable/rounded_rectangle_24dp"
        android:backgroundTint="@color/color_green1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/img_profile" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_message"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:background="@drawable/rounded_rectangle_24dp"
        android:backgroundTint="@color/white"
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        android:gravity="center"
        android:text="Test text!"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@id/textview_blank"
        app:layout_constraintTop_toBottomOf="@id/img_profile_with_icon" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textview_message_tint"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:scaleType="centerCrop"
        android:background="@drawable/rounded_rectangle_24dp"
        android:backgroundTint="@color/color_green1"
        android:paddingStart="20dp"
        android:paddingEnd="20dp"
        android:gravity="center"
        android:text="This textview is tinted!"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@id/textview_message"
        app:layout_constraintTop_toBottomOf="@id/img_profile_with_icon" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/fab_new_chat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_gravity="bottom|end"
        android:text="@string/action_new_chat"
        android:backgroundTint="@color/color_green1"
        app:icon="@drawable/ic_chat_black_outline_24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textview_blank"/>

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/fab_new_contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:layout_gravity="bottom|end"
        android:text="@string/action_new_contact"
        android:backgroundTint="@color/color_green1"
        app:icon="@drawable/ic_newchat_black_24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fab_new_chat"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_filled_chat"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:src="@drawable/ic_chat_black_filled_24dp"
        android:tint="@color/color_green1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_outline_chat"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:src="@drawable/ic_chat_black_outline_24dp"
        android:tint="@color/color_green1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/img_filled_chat"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_filled_group"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:src="@drawable/ic_group_black_filled_24dp"
        android:tint="@color/color_green1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/img_outline_chat"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_filled_people"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:src="@drawable/ic_people_black_filled_24dp"
        android:tint="@color/color_green1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/img_filled_group"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_outline_people"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:src="@drawable/ic_people_black_outline_24dp"
        android:tint="@color/color_green1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/img_filled_people"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/img_chat_unread"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_margin="10dp"
        android:src="@drawable/ic_chat_black_unread_24dp"
        android:tint="@color/color_green1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/img_outline_people"/>

</androidx.constraintlayout.widget.ConstraintLayout>